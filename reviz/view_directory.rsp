<html>
<head>
<title>directory</title>
</head>
<body>

<%unless directory.empty?%>
<h2>directory</h2>
<ul>
<%directory.each {|d| with(d) {
%><li><a href="<%=url%>"><%=name%></a></li>
<%}}
%></ul>
<%end%>

<%unless file.empty?%>
<h2>file</h2>
<ul>
<%file.each {|f| with(f) {
%><li><a href="<%=url%>"><%=name%></a></li>
<%}}
%></ul>
<%end%>

</body>
</html>
